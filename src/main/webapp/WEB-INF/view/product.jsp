<script type="text/javascript">
// card 영역에 입체감 효과를 주기 위한 jquery 코드
$('.card').mouseenter(function() {
	$(this).css('transform','scale(1.05)');
	$(this).css('zIndex','1');
	$(this).css('transition','all 0.8s');
});
$('.card').mouseleave(function() {
	$(this).css('transform','');
	$(this).css('zIndex', '');
	$(this).css('transition','');
});

function Product_List(title) {
	var title_list = new Array("Milton-Roy-div","LMI-div","will-div");
	for(var i=0; i<3; i++){
		
		if(title_list[i] == title ){
			$("." + title_list[i]).css("display", "block");
// 			$("." + title_list[i]).fadeTo(1,1); jquery 애니메이션 효과줄려면 css에서 transition로 시간 주면 천천히 효과적용 가능
		}
		else{
			$("." + title_list[i]).css("display", "none");
		}
	}
	
}


</script>
